<div class="swag">
     <div class="reviews">
     			<p><%= pluralize(@review.votes.count, "vote") %></p>
                <div class="star-rating" data-score= <%= @review.rating %>>  </div> 
	            <strong><%= @review.user.email %> said: </strong><br><%= @review.comment %></p>
	 </div>
 </div>

 <script>
  $('.star-rating').raty({
    path: '/assets/',
    readOnly: true,
    score: function() {
        return $(this).attr('data-score');
    }

  });
</script>