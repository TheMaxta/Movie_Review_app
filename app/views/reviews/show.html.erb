<p id="notice"><%= notice %></p>

<p>
  <strong>Rating:</strong>
  <%= @review.rating %>
</p>

<p>
  <strong>Comment:</strong>
  <%= @review.comment %>
</p>
<p>
	<strong>User</strong>
	<%= @review.user.email %>
</p>
<p>
	<strong>Created</strong>
	<%= @review.created_at.strftime("%b %e, %l:%M %p") %>
</p>

<%= link_to 'Edit', edit_review_path(@review) %> |
<%= link_to 'Back', reviews_path %>
